<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://qconsulting.ca/fo3/ModConfig5.0.xsd">
	<moduleName>Dripping wet</moduleName>
	<installSteps order="Explicit">	
		<installStep name="Choose Your Setup">
			<optionalFileGroups>
				<group name="CORE" type="SelectExactlyOne">
					<plugins order="Explicit">
						<plugin name="CORE">
							<description>
								Esp, Meshes, Textures, Sounds, core scripts.
							</description>
							<image path="Fomod\Screens\ejaculation_1024.jpg"/>
							<conditionFlags>
								<flag name="CoreDefault">True</flag>
							</conditionFlags>
							<files>
								<folder source="00_Core" destination=""/>
							</files>
							<typeDescriptor>
								<type name="Required"/>
							</typeDescriptor>
						</plugin>
					</plugins>
				</group>
				<group name="Optional integration" type="SelectAny">
					<plugins order="Explicit">
						<plugin name="Readme">
							<description>
								Installing integration scripts for optional mods./n Installing scripts without appropriate mods may lead to mod conflicts, CTDs, etc.
							</description>
							<conditionFlags>
								<flag name="Readme">True</flag>
							</conditionFlags>
							<typeDescriptor>
								<type name="Required"/>
							</typeDescriptor>
						</plugin>
						<plugin name="Schlongs of Skyrim(FULL)">
							<description>
							</description>
							<conditionFlags>
								<flag name="SoS">True</flag>
							</conditionFlags>
							<typeDescriptor>
								<type name="Optional"/>
							</typeDescriptor>
						</plugin>
						<plugin name="SexLab Aroused">
							<description>
							</description>
							<conditionFlags>
								<flag name="SlA">True</flag>
							</conditionFlags>
							<typeDescriptor>
								<type name="Optional"/>
							</typeDescriptor>
						</plugin>
					</plugins>   
				</group>
			</optionalFileGroups>
		</installStep>
	</installSteps>
	<conditionalFileInstalls>
		<patterns>
			<pattern>
				<dependencies operator="And">
					<flagDependency flag="SoS" value="True"/>
				</dependencies>
				<files>
					<folder source="01_Optional\SoS\True" destination=""/>
				</files>
			</pattern>
			<pattern>
				<dependencies operator="And">
					<flagDependency flag="SlA" value="True"/>
				</dependencies>
				<files>
					<folder source="01_Optional\SlA\True" destination=""/>
				</files>
			</pattern>
			
			<pattern>
				<dependencies operator="And">
					<flagDependency flag="SoS" value=""/>
				</dependencies>
				<files>
					<folder source="01_Optional\SoS\False" destination=""/>
				</files>
			</pattern>
			<pattern>
				<dependencies operator="And">
					<flagDependency flag="SlA" value=""/>
				</dependencies>
				<files>
					<folder source="01_Optional\SlA\False" destination=""/>
				</files>
			</pattern>
		</patterns>
	</conditionalFileInstalls>
</config>		